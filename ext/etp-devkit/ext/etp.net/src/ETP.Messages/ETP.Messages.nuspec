<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ETP</id>
    <version>$version$</version>
    <title>Energistics Transfer Protocol</title>
    <owners>Energistics</owners>
    <authors>Welly Tambunan, William McKenzie, Bobby Diaz</authors>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0.html</licenseUrl>
    <projectUrl>https://bitbucket.org/energistics/etp.net</projectUrl>
    <iconUrl>https://bitbucket.org/account/energistics/avatar/</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The Energistics Transfer Protocol (ETP) is a specification for an asynchronous message-based data transfer standard used by the Energistics family data formats.</description>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Apache.Avro" version="1.7.7.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin/$configuration$/$id$.pdb" target="lib/net40" />
  </files>
</package>
